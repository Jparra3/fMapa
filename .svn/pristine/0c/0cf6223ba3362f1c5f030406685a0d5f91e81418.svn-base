<?php

namespace entidad;
class EstadoPedidoProducto {
    private $idEstadoPedidoProducto;
    private $estadoPedidoProducto;
    private $estado;
    private $inicial;
    
    function getIdEstadoPedidoProducto() {
        return $this->idEstadoPedidoProducto;
    }

    function getEstadoPedidoProducto() {
        return $this->estadoPedidoProducto;
    }

    function getEstado() {
        return $this->estado;
    }

    function getInicial() {
        return $this->inicial;
    }

    function setIdEstadoPedidoProducto($idEstadoPedidoProducto) {
        $this->idEstadoPedidoProducto = $idEstadoPedidoProducto;
    }

    function setEstadoPedidoProducto($estadoPedidoProducto) {
        $this->estadoPedidoProducto = $estadoPedidoProducto;
    }

    function setEstado($estado) {
        $this->estado = $estado;
    }

    function setInicial($inicial) {
        $this->inicial = $inicial;
    }


}
