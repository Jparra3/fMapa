<?php
namespace entidad;
class Bodega {
    private $idBodega;
    private $bodega;
    private $idOficina;
    private $estado;
    
    function getIdBodega() {
        return $this->idBodega;
    }

    function getBodega() {
        return $this->bodega;
    }

    function getIdOficina() {
        return $this->idOficina;
    }

    function getEstado() {
        return $this->estado;
    }

    function setIdBodega($idBodega) {
        $this->idBodega = $idBodega;
    }

    function setBodega($bodega) {
        $this->bodega = $bodega;
    }

    function setIdOficina($idOficina) {
        $this->idOficina = $idOficina;
    }

    function setEstado($estado) {
        $this->estado = $estado;
    }


}
